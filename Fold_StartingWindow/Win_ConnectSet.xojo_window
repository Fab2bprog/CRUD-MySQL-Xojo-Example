#tag WindowCode
	#tag Event
		Sub Opening()
		  Txt_Host.Text=App.MainDB.Host
		  Txt_Port.Text=App.MainDB.Port.ToString
		  Txt_UserName.Text=App.MainDB.UserName
		  Txt_UserPassword.Text=App.MainDB.Password
		  Txt_DatabaseName.Text=App.MainDB.DatabaseName
		  
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function Touch_Valid(key as String) As Boolean
		  if key=chr(13)  then return true
		  if key=chr(3)    then return true
		  
		  
		End Function
	#tag EndMethod


#tag EndWindowCode

#tag Events Txt_Host
	#tag Event
		Function KeyDown(key As String) As Boolean
		  if Touch_Valid(Key) then
		    FocusNext
		    return True
		  end if
		End Function
	#tag EndEvent
#tag EndEvents
