#tag Events BTN_OK
	#tag Event
		Sub Pressed()
		  
		  App.MainDB.Host =Txt_Host.Text
		  App.MainDB.Port = Txt_Port.Text.ToInt64
		  App.MainDB.DatabaseName = Txt_DatabaseName.Text
		  App.MainDB.UserName = Txt_UserName.Text
		  App.MainDB.Password = Txt_UserPassword.Text
		  
		  Win_ConnectSet.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events BTN_Cancel
	#tag Event
		Sub Pressed()
		  Win_ConnectSet.Close
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events BTN_Test
	#tag Event
		Sub Pressed()
		  Var TestDB as  MySQLCommunityServer
		  
		  TestDB = New MySQLCommunityServer
		  TestDB.Host =Txt_Host.Text
		  TestDB.Port = Txt_Port.Text.ToInt64
		  TestDB.DatabaseName = Txt_DatabaseName.Text
		  TestDB.UserName = Txt_UserName.Text
		  TestDB.Password = Txt_UserPassword.Text
		  Try
		    TestDB.Connect
		    MessageBox("Connection test ok, database connected.")
		    TestDB.Close
		  Catch error As DatabaseException
		    ' DB Connection error
		    MessageBox(error.Message)
		  End Try
		  
		  
		End Sub
	#tag EndEvent
#tag EndEvents
